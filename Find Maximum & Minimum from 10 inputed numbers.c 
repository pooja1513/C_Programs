#include<stdio.h>
#include<conio.h>
int main()
{
	int No=0, Min=0, Max=0, Cnt=0;
	
	printf("\n Enter 10 numbers =");
	
	for(Cnt=1; Cnt<=10; Cnt++)
	{
		printf("\n\t Enter number %d=",Cnt);
		scanf("%d",&No);
		
		if(Cnt==1)
		{
			Min=No;
			Max=No;
			continue;
		}
		
		if(No<Min)
		{
			Min=No;
		}
		
		if(No>Max)
		{
			Max=No;
		}
	}
	
	printf("\n Press any key to get maximum & minimum from given numbers =");
	getche();
	
	printf("\n Maximum of given numbers = %d",Max);
	
	printf("\n Minimum of given numbers = %d", Min);
	
	printf("\n\n Thanks");
	
	getch();
	return 0;
}